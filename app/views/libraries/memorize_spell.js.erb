
<% if @memorized_spell.number_memorized == 1 %>
  viewModel.removeSpell("<%= ViewModels::SpellItem.create_key(@memorized_spell.spell_id, nil) %>");
  viewModel.addSpell(<%= @spells.get_json_for_memorized_spell_id(@memorized_spell.id, @mode).html_safe %>);
<% else %>
  viewModel.updateSpell(<%= @spells.get_json_for_memorized_spell_id(@memorized_spell.id, @mode).html_safe %>);
<% end %>

viewModel.sort();